def imprimir_tabla(numero):
    print(f"\nTabla del {numero}")
    for i in range(1,11):
        resultado = numero * i
        print(f"{numero} por {i} es igual a {resultado}")

# imprimir_tabla(7)
# imprimir_tabla(5)

def contar_vocales(texto):
    vocales = "aeiouAEIOUáéíóúÁÉÍÓÚ"
    contador = 0
    for letra in texto:
        if letra in vocales:
            contador += 1
    return contador

frase = input("Digite una frase: ")
total = contar_vocales(frase)
print(total)